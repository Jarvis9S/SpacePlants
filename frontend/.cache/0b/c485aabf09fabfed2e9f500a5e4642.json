port { signup , login } from \"../actions/account.js\";\nimport fetchStates from \"../reducers/fetchStates.js\";\n\nclass AuthForm extends Component {\n  state = { username: \"\", password: \"\" };\n\n  updateUsername = (event) => {\n    this.setState({ username: event.target.value });\n  };\n  updatePassword = (event) => {\n    this.setState({ password: event.target.value });\n  };\n\n  signup = () => {\n    const { username, password } = this.state;\n\n    this.props.signup({ username, password });\n  };\n\n  login = () => {\n    const { username, password } = this.state;\n\n    this.props.login({ username, password });\n  };\n\n  get Error() {\n    if (this.props.account.status === fetchStates.error) {\n      return <div>{this.props.account.message}</div>;\n    }\n  };\n\n  render() {\n    return (\n      <div>\n        <h2>Log into your garden</h2>\n        <br />\n        <FormGroup>\n          <FormControl\n            type=\"text\"\n            value={this.state.username}\n            placeholder=\"username\"\n            onChange={this.updateUsername}\n          />\n          <br />\n        </FormGroup>\n        <FormGroup>\n          <FormControl\n            type=\"password\"\n            value={this.state.password}\n            placeholder=\"password\"\n            onChange={this.updatePassword}\n          />\n        </FormGroup>\n        <div>\n          <Button onClick={this.login}>Log In</Button>\n          <span> or </span>\n          <Button onClick={this.signup}>Sign Up</Button>\n        </div>\n        <br />\n        {this.Error}\n      </div>\n    );\n  }\n}\n\nexport default connect(\n  ({ account }) => ({ account }),\n  { signup, login }\n)(AuthForm);\n"},"lineCount":null}},"error":null,"hash":"c62c724ee39aa2a0e02eb695d0a696d1","cacheData":{"env":{}}}