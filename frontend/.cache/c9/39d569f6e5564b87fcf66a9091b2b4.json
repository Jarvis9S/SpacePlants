ctor","generated":{"line":126,"column":4},"source":"components/Travel.js","original":{"line":68,"column":6}},{"generated":{"line":126,"column":22},"source":"components/Travel.js","original":{"line":68,"column":24}},{"generated":{"line":126,"column":25},"source":"components/Travel.js","original":{"line":68,"column":27}},{"name":"mapStateToProps","generated":{"line":126,"column":50},"source":"components/Travel.js","original":{"line":68,"column":35}},{"generated":{"line":126,"column":65},"source":"components/Travel.js","original":{"line":68,"column":27}},{"name":"mapDispatchToProps","generated":{"line":126,"column":67},"source":"components/Travel.js","original":{"line":68,"column":52}},{"generated":{"line":126,"column":85},"source":"components/Travel.js","original":{"line":68,"column":27}},{"generated":{"line":126,"column":86},"source":"components/Travel.js","original":{"line":68,"column":0}},{"name":"componentConnector","generated":{"line":128,"column":15},"source":"components/Travel.js","original":{"line":70,"column":15}},{"generated":{"line":128,"column":33},"source":"components/Travel.js","original":{"line":70,"column":33}},{"name":"Travel","generated":{"line":128,"column":34},"source":"components/Travel.js","original":{"line":70,"column":34}},{"generated":{"line":128,"column":40},"source":"components/Travel.js","original":{"line":70,"column":33}}],"sources":{"components/Travel.js":"import React, {Component} from \"react\";\nimport { connect } from \"react-redux\";\nimport { travelActionCreator } from \"../actions/travel\";\n\nconst MINIMUM_DELAY = 3000;\n\nclass Travel extends Component {\n    timer = null;\n\n    componentDidMount() {\n        this.fetchNextTravel();\n    }\n\n    componentWillUnmount() {\n        clearTimeout(this.timer);\n    }\n    \n    fetchTravel = () => {\n        fetch(\"http://localhost:3000/travel\")\n        .then(response => response.json())\n        .then (json => {\n            this.props.dispatchTravel(json.travel);\n        })\n        .catch(error => console.error(\"error\", error))\n    };\n\n    fetchNextTravel = () => {\n        this.fetchTravel();\n\n        let delay = new Date(this.props.travel.expiration).getTime() - new Date().getTime;\n\n        if (delay < MINIMUM_DELAY) {\n            delay = MINIMUM_DELAY;\n        };\n\n        this.timer = setTimeout(() => this.fetchNextTravel(), delay);\n    }\n\n    render() {\n\n        const { travel } = this.props;\n\n        return (\n            <div>\n                <h3>Travel {travel.travelId}. Expires on:</h3>\n                <h4>{new Date(travel.expiration).toString()}</h4>\n                <h1>{Date()}</h1>\n            </div>\n        )\n    }\n}\n\nconst mapStateToProps = (state) => {\n    const travel = state.travel;\n    return { travel };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n    return {\n        dispatchTravel: (travel) => {\n            dispatch(\n                travelActionCreator(travel)\n            )\n        }\n    } \n};\n\nconst componentConnector = connect(mapStateToProps, mapDispatchToProps);\n\nexport default componentConnector(Travel);"},"lineCount":null}},"error":null,"hash":"9c8336517ce8af64b77cc4bd2be5c2e5","cacheData":{"env":{}}}