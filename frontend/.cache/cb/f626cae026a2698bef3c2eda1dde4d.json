h({ type: ACCOUNT.FETCH_ERROR, message: json.message });\n        } else {\n          dispatch({ type: SUCCESS_TYPE, ...json });\n        }\n      })\n      .catch((error) =>\n        dispatch({\n          type: ACCOUNT.FETCH_ERROR,\n          message: error.message,\n        })\n      );\n  };\n\nexport const signup = ({ username, password }) =>\n  fetchFromAccount({\n    endpoint: \"signup\",\n    options: {\n      method: \"POST\",\n      body: JSON.stringify({ username, password }),\n      headers: { \"Content-Type\": \"application/json\" },\n      // credentials: \"include\"\n    },\n    SUCCESS_TYPE: ACCOUNT.FETCH_SUCCESS,\n  });\n\nexport const login = ({ username, password }) =>\n  fetchFromAccount({\n    endpoint: \"login\",\n    options: {\n      method: \"POST\",\n      body: JSON.stringify({ username, password }),\n      headers: { \"Content-Type\": \"application/json\" },\n      // credentials: \"include\"\n    },\n    SUCCESS_TYPE: ACCOUNT.FETCH_SUCCESS,\n  });\n\nexport const logout = () =>\n  fetchFromAccount({\n    endpoint: \"logout\",\n    options: {},\n    SUCCESS_TYPE: ACCOUNT.FETCH_LOGOUT_SUCCESS,\n  });\n"},"lineCount":null}},"error":null,"hash":"5892faa23aa8c9aeb24cfe7e2cef6b91","cacheData":{"env":{}}}